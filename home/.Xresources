
XTerm*boldColors: false
XTerm*locale: true
XTerm*geometry: 120x40
XTerm*faceName: Source Code Pro Medium:size=16
!!XTerm*faceName: Roboto Mono:size=14
!!XTerm*faceName: Droid Sans Mono:size=14
XTerm*metaSendsEscape: true
XTerm*saveLines: 2000
XTerm*termName: xterm-256color
XTerm*cursorColor: orange
XTerm*cursorBlink: true

URxvt*background: #002b36
URxvt*foreground: #657b83
URxvt*intensityStyles: false
URxvt*geometry: 120x40
URxvt*scrollBar: false
URxvt*saveLines: 2000
URxvt*buffered: true
URxvt*termName: rxvt-256color
URxvt*font: xft:Source Code Pro Medium:size=16
!!URxvt*font: xft:Roboto Mono:size=14
!!URxvt*font: xft:Droid Sans Mono:size=14

Xft.dpi: 108
Xft.autohint: 0
Xft.lcdfilter:  lcddefault
Xft.hintstyle:  hintfull
Xft.hinting: 1
Xft.antialias: 1
Xft.rgba: rgb

